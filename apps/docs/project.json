{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/docs",
  "projectType": "application",
  "tags": [],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitepress dev apps/docs",
        "cwd": "."
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "command": "vitepress build apps/docs",
        "cwd": "."
      },
      "outputs": ["{projectRoot}/.vitepress/dist"]
    },
    "preview": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "vitepress preview apps/docs",
        "cwd": "."
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "vitepress preview apps/docs --port 4173",
        "cwd": "."
      }
    }
  }
}
