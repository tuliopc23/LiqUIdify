{
  "name": "@liquidify/storybook-app",
  "version": "1.3.3",
  "private": true,
  "type": "module",
  "description": "LiqUIdify Storybook - Interactive component playground",
  "scripts": {
    "dev": "storybook dev -p 6006 -c .storybook",
    "build": "storybook build -c .storybook -o storybook-static",
    "preview": "serve storybook-static -l 6006",
    "verify": "storybook build -c .storybook -o storybook-static && node -e \"const fs=require('fs');if(!fs.existsSync('storybook-static/index.html')){process.exit(1)}\"",
    "typecheck": "bunx tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@liquidify/components": "workspace:*",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@storybook/addon-a11y": "^9.1.2",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-links": "^9.1.2",
    "@storybook/builder-vite": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "prism-react-renderer": "^2.4.1",
    "serve": "^14.2.4",
    "storybook": "^9.1.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  }
}
