{
  "name": "@liquidify/storybook-app",
  "version": "1.3.6",
  "private": true,
  "type": "module",
  "description": "LiqUIdify Storybook - Interactive component playground",
  "scripts": {
    "dev": "storybook dev -p 6006 -c .storybook",
    "build": "bunx storybook build -c .storybook -o storybook-static",
    "prepreview": "bun run build",
    "preview": "serve storybook-static -l 6006",
    "verify": "bun run --cwd ../../libs/components build && storybook build -c .storybook -o storybook-static && node -e \"const fs=require('fs');if(!fs.existsSync('storybook-static/index.html')){process.exit(1)}\"",
    "typecheck": "bunx tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "react": "^19.1.1"
  },
  "devDependencies": {
    "@storybook/addon-themes": "^9.1.3",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-links": "^9.1.3",
    "@storybook/builder-vite": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@types/react": "^19.1.10",
    "serve": "^14.2.4",
    "storybook": "^9.1.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
