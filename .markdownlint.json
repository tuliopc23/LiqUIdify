{
	"extends": "markdownlint/style/prettier",
	"default": true,
	"MD001": false,
	"MD003": {
		"style": "atx"
	},
	"MD007": {
		"indent": 2
	},
	"MD010": {
		"code_blocks": false,
		"spaces_per_tab": 2
	},
	"MD013": {
		"line_length": 120,
		"heading_line_length": 120,
		"code_block_line_length": 120,
		"code_blocks": false,
		"tables": false,
		"headings": false,
		"headers": false,
		"stern": false
	},
	"MD021": false,
	"MD024": {
		"siblings_only": true,
		"allow_different_nesting": true
	},
	"MD025": {
		"front_matter_title": "^\\s*title\\s*[:=]"
	},
	"MD026": {
		"punctuation": ".,;:!?"
	},
	"MD028": false,
	"MD029": {
		"style": "ordered"
	},
	"MD030": {
		"ul_single": 1,
		"ol_single": 1,
		"ul_multi": 1,
		"ol_multi": 1
	},
	"MD033": false,
	"MD034": false,
	"MD036": false,
	"MD041": false,
	"MD046": {
		"style": "fenced"
	},
	"MD047": false,
	"MD049": {
		"style": "underscore"
	},
	"MD050": {
		"style": "asterisk"
	},
	"MD051": false,
	"MD052": false,
	"MD053": false,
	"MD054": false,
	"MD055": false,
	"MD056": false
}
