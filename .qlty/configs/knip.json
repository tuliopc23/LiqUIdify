{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "apps/docs/src/main.tsx",
    "apps/storybook/src/main.ts",
    "libs/components/src/index.ts",
    "scripts/**/*.ts"
  ],
  "project": ["apps/**/*.ts", "apps/**/*.tsx", "libs/**/*.ts", "libs/**/*.tsx"],
  "ignore": ["dist", "node_modules", ".nx", "**/*.d.ts"],
  "ignoreDependencies": [
    "nx",
    "@nx/vite",
    "@nx/playwright",
    "@nx/react",
    "@nx/storybook",
    "vite",
    "vitest",
    "vite-plugin-dts",
    "@vitejs/plugin-react",
    "@swc/core",
    "@swc-node/register",
    "rolldown-vite",
    "terser",
    "tsx",
    "jiti",
    "lightningcss"
  ],
  "workspaces": {
    "apps/docs": {
      "entry": "src/main.tsx"
    },
    "apps/storybook": {
      "entry": "src/main.ts"
    },
    "libs/components": {
      "entry": "src/index.ts"
    }
  }
}
