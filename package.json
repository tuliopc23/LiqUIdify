{
  "name": "@tuliocunha23/liquidui",
  "version": "1.0.16",
  "type": "module",
  "description": "ðŸš€ Physics-based liquid glass library with content-aware adaptation, magnetic interactions, and 30+ glass effects. TypeScript-first, accessibility-ready.",
  "main": "dist/liquidui.umd.js",
  "module": "dist/liquidui.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/liquidui.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/liquidui.es.js",
      "require": "./dist/liquidui.umd.js"
    },
    "./styles": "./dist/liquidui.css",
    "./css": "./dist/liquidui.css"
  },
  "scripts": {
    "build": "npm run clean && npm run build:css && vite build",
    "build:css": "postcss src/styles/glass.css -o dist/liquidui.css",
    "build:watch": "vite build --watch",
    "clean": "rm -rf dist",
    "dev": "vite",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "npx storybook build",
    "size": "npm run build && bundlesize"
  },
  "keywords": [
    "glassmorphism",
    "liquidui",
    "liquid-glass",
    "ui-library",
    "physics",
    "content-aware",
    "magnetic-hover",
    "spring-animations",
    "accessibility",
    "typescript",
    "react",
    "webgl",
    "design-system",
    "components",
    "tailwind"
  ],
  "author": {
    "name": "Tulio Pinheiro Cunha",
    "email": "tulio@example.com",
    "url": "https://tulio.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuliopc23/LiquidiUI.git"
  },
  "bugs": {
    "url": "https://github.com/tuliopc23/LiquidiUI/issues"
  },
  "homepage": "https://liquidui.dev",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tuliopc23"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
