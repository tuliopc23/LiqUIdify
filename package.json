    "web-vitals": "^5.0.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "bunx oxlint --fix",
      "prettier --write"
    ]
  }
