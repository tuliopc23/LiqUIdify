{
  "name": "liquidify",
  "version": "1.2.4",
  "type": "module",
  "description": "ðŸš€ Production-ready React component library with glassmorphism design and physics-based interactions. 40+ components, TypeScript-first, accessibility-ready.",
  "main": "./dist/libs/components/index.cjs",
  "module": "./dist/libs/components/index.mjs",
  "types": "./dist/libs/components/index.d.ts",
  "style": "./dist/libs/components/liquidui.css",
  "exports": {
    ".": {
      "types": "./dist/libs/components/index.d.ts",
      "import": "./dist/libs/components/index.mjs",
      "require": "./dist/libs/components/index.cjs",
      "style": "./dist/libs/components/liquidui.css"
    },
    "./core": {
      "types": "./dist/libs/components/bundles/core.d.ts",
      "modern": "./dist/libs/components/core.mjs",
      "import": "./dist/libs/components/core.mjs",
      "require": "./dist/libs/components/cjs/core.cjs"
    },
    "./animations": {
      "types": "./dist/libs/components/bundles/animations.d.ts",
      "import": "./dist/libs/components/animations.mjs",
      "require": "./dist/libs/components/cjs/animations.cjs"
    },
    "./advanced": {
      "types": "./dist/libs/components/bundles/advanced.d.ts",
      "import": "./dist/libs/components/advanced.mjs",
      "require": "./dist/libs/components/cjs/advanced.cjs"
    },
    "./forms": {
      "types": "./dist/libs/components/bundles/forms.d.ts",
      "import": "./dist/libs/components/forms.mjs",
      "require": "./dist/libs/components/cjs/forms.cjs"
    },
    "./layout": {
      "types": "./dist/libs/components/bundles/layout.d.ts",
      "import": "./dist/libs/components/layout.mjs",
      "require": "./dist/libs/components/cjs/layout.cjs"
    },
    "./accessibility": {
      "types": "./dist/libs/components/bundles/accessibility.d.ts",
      "import": "./dist/libs/components/accessibility.mjs",
      "require": "./dist/libs/components/cjs/accessibility.cjs"
    },
    "./feedback": {
      "types": "./dist/libs/components/bundles/feedback.d.ts",
      "import": "./dist/libs/components/feedback.mjs",
      "require": "./dist/libs/components/cjs/feedback.cjs"
    },
    "./navigation": {
      "types": "./dist/libs/components/bundles/navigation.d.ts",
      "import": "./dist/libs/components/navigation.mjs",
      "require": "./dist/libs/components/cjs/navigation.cjs"
    },
    "./physics": {
      "types": "./dist/libs/components/bundles/physics.d.ts",
      "import": "./dist/libs/components/physics.mjs",
      "require": "./dist/libs/components/cjs/physics.cjs"
    },
    "./ssr": {
      "types": "./dist/libs/components/bundles/ssr.d.ts",
      "import": "./dist/libs/components/ssr.mjs",
      "require": "./dist/libs/components/cjs/ssr.cjs"
    },
    "./button": {
      "types": "./dist/libs/components/components/glass-button/index.d.ts",
      "import": "./dist/libs/components/components/button.mjs",
      "require": "./dist/libs/components/cjs/components/button.cjs"
    },
    "./card": {
      "types": "./dist/libs/components/components/glass-card/index.d.ts",
      "import": "./dist/libs/components/components/card.mjs",
      "require": "./dist/libs/components/cjs/components/card.cjs"
    },
    "./input": {
      "types": "./dist/libs/components/components/glass-input/index.d.ts",
      "import": "./dist/libs/components/components/input.mjs",
      "require": "./dist/libs/components/cjs/components/input.cjs"
    },
    "./modal": {
      "types": "./dist/libs/components/components/glass-modal/index.d.ts",
      "import": "./dist/libs/components/components/modal.mjs",
      "require": "./dist/libs/components/cjs/components/modal.cjs"
    },
    "./tokens": {
      "types": "./dist/libs/components/tokens/index.d.ts",
      "import": "./dist/libs/components/tokens.mjs",
      "require": "./dist/libs/components/cjs/tokens.cjs"
    },
    "./context": {
      "types": "./dist/libs/components/providers/index.d.ts",
      "import": "./dist/libs/components/providers.mjs",
      "require": "./dist/libs/components/cjs/providers.cjs"
    },
    "./styles": "./dist/libs/components/liquidui.css",
    "./css": "./dist/libs/components/liquidui.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bun run build:lib",
    "build:lib": "nx build components && bun run post-build",
    "build:storybook": "nx build-storybook storybook",
    "build:css": "nx build:css components",
    "build:modern": "nx build components --target es2018",
    "build:watch": "nx build components --watch",
    "build:all": "nx run-many -t build --parallel=3",
    "clean": "rm -rf dist",
    "clean:node": "rm -rf node_modules package-lock.json && bun install",
    "dev": "nx serve components",
    "dev:storybook": "nx storybook storybook",
    "format": "biome format --write .",
    "lint": "nx run-many -t lint",
    "lint:fix": "biome check --fix --unsafe .",
    "lint:affected": "nx affected -t lint",
    "post-build": "bun scripts/post-build.ts",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "storybook": "nx storybook storybook",
    "test": "bun test",
    "test:ci": "bun test --coverage",
    "test:affected": "nx affected -t test",
    "test:e2e": "nx e2e storybook",
    "test:e2e:ui": "playwright test --ui",
    "type-check": "nx type-check components",
    "type-check:strict": "tsc --noEmit --strict --noImplicitAny --strictNullChecks",
    "graph": "nx graph",
    "affected:graph": "nx affected:graph",
    "docs:dev": "nx dev docs",
    "docs:build": "nx build docs",
    "docs:preview": "nx preview docs"
  },
  "keywords": [
    "glassmorphism",
    "liquidui",
    "liquid-glass",
    "ui-library",
    "physics",
    "content-aware",
    "magnetic-hover",
    "spring-animations",
    "accessibility",
    "typescript",
    "react",
    "webgl",
    "design-system",
    "components",
    "tailwind"
  ],
  "author": {
    "name": "Tulio Pinheiro Cunha",
    "email": "contact@tuliocunha.dev",
    "url": "https://tuliocunha.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuliopc23/LiqUIdify.git"
  },
  "bugs": {
    "url": "https://github.com/tuliopc23/LiqUIdify/issues"
  },
  "homepage": "https://liquidify-storybook.vercel.app",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tuliopc23"
  },
  "files": [
    "dist/libs/components/**/*.{js,mjs,cjs,d.ts,css}",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css",
    "src/styles/**"
  ],
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "knip": "^5.62.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@nx/playwright": "^21.3.9",
    "@nx/react": "^21.3.9",
    "@nx/storybook": "^21.3.9",
    "@nx/vite": "^21.3.9",
    "@playwright/test": "^1.54.1",
    "@storybook/react": "^9.0.18",
    "@swc-node/register": "~1.9.2",
    "@swc/core": "~1.5.29",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "^1.2.19",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "axe-core": "^4.10.3",
    "jiti": "2.4.2",
    "lightningcss": "^1.30.1",
    "nx": "21.3.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rolldown-vite": "^7.0.12",
    "semantic-release": "^24.2.7",
    "storybook": "^9.0.18",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "nx": {}
}
