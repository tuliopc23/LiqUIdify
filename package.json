  },
  "homepage": "https://liquidify-fw9pi7oj6-tulio-pinheiro-cunha-s-projects.vercel.app",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tuliopc23"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
