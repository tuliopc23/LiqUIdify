    "prettier": "^3.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "semantic-release": "^24.2.6",
    "storybook": "^9.0.13",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.43.1",
    "tslib": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "vitest-canvas-mock": "^0.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not IE 11"
  ]
}
