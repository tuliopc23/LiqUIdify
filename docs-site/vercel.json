{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd .. && npm run docs:prepare && cd docs-site && npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": "out",
  "installCommand": "cd .. && npm ci && cd docs-site && npm ci",
  "framework": "nextjs",
  "regions": [
    "iad1"
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/docs",
      "destination": "/",
      "permanent": false
    }
  ]
}